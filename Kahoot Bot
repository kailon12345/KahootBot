[cite_start]-- SCOREBOARD FUNCTIONS [cite: 74]
local function GetSortedScores()
    local scores = {}
    for name, s in pairs(PlayerScores) do table.insert(scores, {n = name, s = s}) end
    table.sort(scores, function(a, b) return a.s > b.s end)
    return scores
end

local function ShowCurrentTop3()
    if MyRole ~= "scoreboard" then return end
    local scores = GetSortedScores()
    if #scores == 0 then return end
    
    [cite_start]local msg = "" [cite: 75]
    for i = 1, math.min(3, #scores) do
        local ordinal = (i==1 and "1st") or (i==2 and "2nd") or (i==3 and "3rd")
        [cite_start]msg = msg .. ordinal .. " " .. scores[i].n .. " (" .. scores[i].s .. ") | " [cite: 75, 76]
    end
    Say(msg)
end

local function CelebrateWinners()
    if MyRole ~= "scoreboard" then return end
    local scores = GetSortedScores()
    if #scores == 0 then return end

    for i = 1, math.min(3, #scores) do
        task.wait(1.5)
        if i == 1 then
            [cite_start]Say("üèÜ " .. scores[i].n .. " got 1st place with " .. scores[i].s .. " points üèÜ") [cite: 76, 77]
        elseif i == 2 then
            [cite_start]Say("ü•à " .. scores[i].n .. " got 2nd place with " .. scores[i].s .. " points ü•à") [cite: 77]
        elseif i == 3 then
            [cite_start]Say("ü•â " .. scores[i].n .. " got 3rd place with " .. scores[i].s .. " points ü•â") [cite: 77]
        end
    end
    
    [cite_start]task.wait(2) [cite: 78]
    Say("Acknowledging all participants...")
    
    for _, data in ipairs(scores) do
        local pName = data.n
        local targetPlayer = nil
        for _, p in pairs(Players:GetPlayers()) do
            [cite_start]if p.DisplayName == pName or p.Name == pName then targetPlayer = p break end [cite: 78, 79]
        end
        
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local tRoot = targetPlayer.Character.HumanoidRootPart
            local frontPos = tRoot.CFrame * CFrame.new(0, 0, -4) 
            local lookAt = CFrame.lookAt(frontPos.Position, tRoot.Position)
            
            [cite_start]Teleport(lookAt) [cite: 80]
            task.wait(0.2)
            Say("/e point")
            [cite_start]task.wait(1.5) [cite: 80, 81]
        end
    end
    
    [cite_start]if PerchPosition then Teleport(PerchPosition) end [cite: 81]
    
    [cite_start]PlayerScores = {} [cite: 81]
    [cite_start]AnswerSubmissions = {} [cite: 81]
end

[cite_start]-- HOST SEQUENCE [cite: 81]
local function StartQuiz(length)
    if MyRole ~= "host" then return end
    GameActive = true
    [cite_start]PlayerScores = {} [cite: 81, 82]
    local UsedQuestions = {}
    local rounds = math.clamp(tonumber(length) or #Questions, 1, #Questions)
    
    [cite_start]Say("Welcome to Schweins Kahoot!") [cite: 82]
    task.wait(3)
    [cite_start]Say("Introduce yourself bots") [cite: 82]
    task.wait(32) 
    
    [cite_start]Say("Beginning " .. rounds .. " rounds. Answer by typing 1, 2, 3, or 4!") [cite: 82, 83]
    task.wait(4)
    
    for i = 1, rounds do
        if not GameActive then break end
        local pick
        repeat pick = math.random(1, #Questions) until not UsedQuestions[pick]
        UsedQuestions[pick] = true
        
        local qData = Questions[pick]
        [cite_start]Say("Question " .. i .. ": " .. qData.q) [cite: 83, 84]
        task.wait(4)
        
        [cite_start]Say("Options: (1) " .. qData.a[1] .. " (2) " .. qData.a[2] .. " (3) " .. qData.a[3] .. " (4) " .. qData.a[4]) [cite: 84]
        task.wait(8)
        
        [cite_start]Say("The correct answer was Option " .. qData.c .. ": " .. qData.a[qData.c]) [cite: 84]
        task.wait(3)
    
        [cite_start]Say("Checking the leaderboard...") [cite: 84, 85]
        task.wait(5) 
    end
    
    if GameActive then
        GameActive = false
        [cite_start]Say("üèÜ THE QUIZ HAS ENDED! THANKS FOR PLAYING! üèÜ") [cite: 85]
    end
end

[cite_start]-- CHAT HANDLER [cite: 85]
local function OnChat(player, message)
    local msg = message:lower()
    local isOwner = (player.UserId == getgenv().ownerID or player.Name == getgenv().owner)

    [cite_start]if msg == getgenv().prefix .. "check" and MyRole then [cite: 85, 86]
        [cite_start]Say("Bot: " .. LocalPlayer.Name .. " | Role: " .. MyRole) [cite: 86, 87]
    end

    if isOwner then
        [cite_start]if msg:sub(1, #getgenv().prefix + 5) == getgenv().prefix .. "start" then [cite: 87]
            local args = msg:split(" ")
            [cite_start]task.spawn(function() StartQuiz(args[2]) end) [cite: 87]
            
        elseif msg == getgenv().prefix .. "end" then
            [cite_start]GameActive = false [cite: 87, 88]
            PlayerScores = {}
            AnswerSubmissions = {}
            [cite_start]if MyRole == "host" then Say("‚èπÔ∏è Game Forced Ended. Scores Reset.") end [cite: 88]
            
        [cite_start]elseif msg == getgenv().prefix .. "setup" then [cite: 88]
             local ownerChar = player.Character
             [cite_start]if ownerChar then [cite: 88, 89]
                local root = ownerChar.HumanoidRootPart.CFrame
                local startPos = (root * CFrame.new(0, 0, -15)).Position 
                
                [cite_start]if MyRole == "host" then Teleport(CFrame.new(startPos + Vector3.new(-16, 0, 0))) [cite: 89]
                [cite_start]elseif MyRole == "answer1" then Teleport(CFrame.new(startPos + Vector3.new(-8, 0, 0))) [cite: 90]
                [cite_start]elseif MyRole == "answer2" then Teleport(CFrame.new(startPos + Vector3.new(0, 0, 0))) [cite: 90]
                [cite_start]elseif MyRole == "answer3" then Teleport(CFrame.new(startPos + Vector3.new(8, 0, 0))) [cite: 90]
                [cite_start]elseif MyRole == "answer4" then Teleport(CFrame.new(startPos + Vector3.new(16, 0, 0))) [cite: 90]
                elseif MyRole == "scoreboard" then 
                    task.wait(0.6) 
                    [cite_start]PerchPosition = CFrame.new(startPos + Vector3.new(0, 10, 5)) [cite: 91]
                    Teleport(PerchPosition) 
                end
             [cite_start]end [cite: 91, 92]
        end
    end

    [cite_start]-- BOT RESPONSES [cite: 92]
    if message == "Introduce yourself bots" then
        [cite_start]if MyRole == "answer1" then task.wait(5) Say("I am Answer Bot 1!") Say("/e wave") [cite: 92]
        [cite_start]elseif MyRole == "answer2" then task.wait(10) Say("I am Answer Bot 2!") Say("/e wave") [cite: 92]
        [cite_start]elseif MyRole == "answer3" then task.wait(15) Say("I am Answer Bot 3!") Say("/e wave") [cite: 92]
        [cite_start]elseif MyRole == "answer4" then task.wait(20) Say("I am Answer Bot 4!") Say("/e wave") [cite: 92, 93]
        [cite_start]elseif MyRole == "scoreboard" then task.wait(25) Say("And I am the Scoreboard!") Say("/e wave") end [cite: 93]
    end

    if message:find("Options:") then
        AnswerSubmissions = {}
        QuestionStartTime = tick()
        for _, q in pairs(Questions) do
            [cite_start]if message:find(q.a[1]) and message:find(q.a[4]) then [cite: 93, 94]
                if MyRole and MyRole:find("answer") then
                    local botIdx = tonumber(MyRole:match("%d+"))
                    task.wait(math.random(1, 3)) 
                    [cite_start]Say(q.a[botIdx]) [cite: 94]
                end
                break
            end
        end
    end

    [cite_start]if message:find("The correct answer was Option") and MyRole == "scoreboard" then [cite: 95]
        local correct = tonumber(message:match("Option (%d+)"))
        local winners = {}
        [cite_start]for name, data in pairs(AnswerSubmissions) do [cite: 95, 96]
            [cite_start]if data.ans == correct then table.insert(winners, {n = name, t = data.time}) end [cite: 96]
        end
        table.sort(winners, function(a, b) return a.t < b.t end)
        for i, v in ipairs(winners) do
            [cite_start]PlayerScores[v.n] = (PlayerScores[v.n] or 0) + math.max(1, 11 - i) [cite: 96]
