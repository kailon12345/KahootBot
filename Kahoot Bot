[cite_start]-- ROLE ASSIGNMENT [cite: 2]
local MyRole = nil
for i, botName in pairs(getgenv().bots) do
    if LocalPlayer.Name:lower() == botName:lower() then
        if i == 1 then MyRole = "host"
        elseif i == 2 then MyRole = "answer1"
        elseif i == 3 then MyRole = "answer2"
        [cite_start]elseif i == 4 then MyRole = "answer3" [cite: 2, 3]
        [cite_start]elseif i == 5 then MyRole = "answer4" [cite: 3]
        [cite_start]elseif i == 6 then MyRole = "scoreboard" [cite: 3]
        end
        break
    end
end

[cite_start]if not MyRole and LocalPlayer.Name ~= getgenv().owner then return end [cite: 3]

[cite_start]-- CHAT LOGIC [cite: 3]
local function Say(msg)
    if not msg then return end
    local cleanMsg = tostring(msg)
    local sayMessage = ReplicatedStorage:FindFirstChild("SayMessageRequest", true)
    if sayMessage then
        [cite_start]sayMessage:FireServer(cleanMsg, "All") [cite: 4]
    else
        local general = TextChatService:FindFirstChild("RBXGeneral", true)
        if general and general:IsA("TextChannel") then
            [cite_start]general:SendAsync(cleanMsg) [cite: 4]
        end
    end
end

[cite_start]-- TELEPORT LOGIC [cite: 4]
local function Teleport(cframe)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        [cite_start]LocalPlayer.Character.HumanoidRootPart.CFrame = cframe [cite: 4]
        
        [cite_start]if MyRole == "scoreboard" then [cite: 5]
            if workspace:FindFirstChild(LocalPlayer.Name .. "_Platform") then 
                [cite_start]workspace[LocalPlayer.Name .. "_Platform"]:Destroy() [cite: 5]
            end
            
            [cite_start]if cframe.Position.Y > (LocalPlayer.Character.HumanoidRootPart.Position.Y + 2) or cframe.Position.Y > 5 then [cite: 6]
                 local plat = Instance.new("Part")
                 plat.Name = LocalPlayer.Name .. "_Platform"
                 plat.Size = Vector3.new(4, 1, 4)
                 [cite_start]plat.CFrame = CFrame.new(cframe.Position - Vector3.new(0, 3.5, 0)) [cite: 6, 7]
                 plat.Transparency = 1
                 plat.CanCollide = true
                 plat.Anchored = true
                 [cite_start]plat.Parent = workspace [cite: 7]
            end
        [cite_start]end [cite: 8]
    end
end

