    [cite_start]if message == "Checking the leaderboard..." then [cite: 96, 97]
        ShowCurrentTop3()
    end
    
    [cite_start]if message == "üèÜ THE QUIZ HAS ENDED! THANKS FOR PLAYING! üèÜ" then [cite: 97, 98]
