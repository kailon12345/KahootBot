-- 6. CELEBRATION FIX
local function Celebrate()
    if MyRole ~= "scoreboard" then return end
    IsDancing = false
    local s = {}
    for name, score in pairs(PlayerScores) do table.insert(s, {n = name, s = score}) end
    table.sort(s, function(a, b) return a.s > b.s end)
    
    if #s == 0 then IsDancing = true return end

    for i = 1, math.min(3, #s) do
        task.wait(1.5)
        local emoji = (i==1 and "üèÜ") or (i==2 and "ü•à") or (i==3 and "ü•â")
        Say(emoji .. " " .. s[i].n .. " got " .. i .. " place with " .. s[i].s .. " points " .. emoji)
    end
    
    task.wait(2)
    for _, data in ipairs(s) do
        local target = nil
        for _, p in pairs(Players:GetPlayers()) do
            if p.DisplayName == data.n or p.Name == data.n then target = p break end
        end
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            local tRoot = target.Character.HumanoidRootPart
            -- Teleport to face the winner
            Teleport(CFrame.lookAt(tRoot.CFrame * CFrame.new(0, 0, -4).Position, tRoot.Position))
            task.wait(0.3) Say("/e point") task.wait(1.5)
        end
    end
    
    -- Teleport back to Perch and restart dance
    if PerchPosition then 
        Teleport(PerchPosition) 
        task.wait(1)
        IsDancing = true 
        Say("/e dance2")
    end
    PlayerScores = {} AnswerSubmissions = {}
end

-- 7. CHAT & COMMANDS
Players.LocalPlayer.Chatted:Connect(function(message)
    local msg = message:lower()
    local isOwner = (LocalPlayer.UserId == getgenv().ownerID)

    if isOwner then
        if msg == ";setup" then
            local ownerChar = Players:GetPlayerByUserId(getgenv().ownerID).Character
            if ownerChar then
                local root = ownerChar.HumanoidRootPart.CFrame
                -- Calculate position 15 studs forward, but rotate CFrame 180 degrees to face owner
                local spawnPos = root * CFrame.new(0, 0, -15) * CFrame.Angles(0, math.rad(180), 0)
                
                if MyRole == "host" then Teleport(spawnPos * CFrame.new(-16, 0, 0))
                elseif MyRole:find("answer") then
                    local bIdx = tonumber(MyRole:match("%d+"))
                    Teleport(spawnPos * CFrame.new(-16 + (bIdx * 8), 0, 0))
                elseif MyRole == "scoreboard" then 
                    PerchPosition = spawnPos * CFrame.new(0, 10, 5)
                    Teleport(PerchPosition) 
                end
            end
        elseif msg == ";end" then
            IsDancing = false
            if CoreGui:FindFirstChild("SchweinsBotUI") then CoreGui.SchweinsBotUI:Destroy() end
        end
    end

    -- Trigger Scoreboard Dance only after intro
    if message == "And I am the Scoreboard!" and MyRole == "scoreboard" then
        task.wait(2)
        IsDancing = true
        Say("/e dance2")
    end
    
    -- [Rest of game logic from previous version]
end)
